<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.tonyshkurenko.slidinguppanelsetup">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".SlidingUpPanelLibraryActivity">
    </activity>
    <activity android:name=".BottomSheetBehaviorActivity">
    </activity>
    <activity android:name=".BottomSheetDialogFragmentActivity">
    </activity>
    <activity android:name=".BottomSheetDialogActivity">
    </activity>
    <activity android:name=".ThreeStatesBehaviorActivity">
    </activity>
  </application>

</manifest>